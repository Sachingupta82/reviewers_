{
  "version": 2,
  "builds": [
    {
      "src": "dist/reviewer-management-system/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { 
      "src": "/assets/(.*)", 
      "dest": "/assets/$1" 
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    { 
      "src": "/(.*)", 
      "dest": "/index.html" 
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist/reviewer-management-system",
  "framework": "angular"
}