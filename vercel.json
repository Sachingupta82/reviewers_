{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/reviewer-management-system",
  "framework": "create-react-app",
  "routes": [
    {
      "src": "/(assets|images)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/(.+\\.[a-zA-Z0-9]+)$",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}